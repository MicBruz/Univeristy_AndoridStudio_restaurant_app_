<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/map2"
    android:name="com.google.android.gms.maps.SupportMapFragment"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MapsFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/textView_godziny_otwarcia"
            android:layout_width="179dp"
            android:layout_height="28dp"
            android:layout_marginTop="196dp"
            android:text="Godziny otwarcia:"
            android:textSize="20dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.931"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView_godziny1"
            android:layout_width="183dp"
            android:layout_height="24dp"
            android:layout_marginTop="8dp"
            android:text="e-mail: info@affogato.pl"
            android:textSize="16dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.947"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView_godziny_otwarcia" />

        <TextView
            android:id="@+id/textView_godziny2"
            android:layout_width="138dp"
            android:layout_height="24dp"
            android:layout_marginTop="8dp"
            android:text="tel. 42 630 03 00"
            android:textSize="16dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.791"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView_godziny1" />

        <TextView
            android:id="@+id/textView_tytul"
            android:layout_width="407dp"
            android:layout_height="67dp"
            android:layout_marginTop="4dp"
            android:textSize="26dp"
            android:text="KONTAKT"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


        <TextView
            android:id="@+id/textView_adres"
            android:layout_width="236dp"
            android:layout_height="30dp"
            android:layout_marginTop="320dp"
            android:text="ŁÓDŹ, PIOTRKOWSKA 144"
            android:textSize="20dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.496"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView_stolik"
            android:layout_width="179dp"
            android:layout_height="28dp"
            android:layout_marginTop="88dp"
            android:text="Rezerwacja stolika:"
            android:textSize="20dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.068"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView_stolik_tel"
            android:layout_width="138dp"
            android:layout_height="24dp"
            android:layout_marginTop="6dp"
            android:text="tel. 42 630 03 00"
            android:textSize="16dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.068"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView_stolik" />

        <TextView
            android:id="@+id/textView_email"
            android:layout_width="197dp"
            android:layout_height="30dp"
            android:layout_marginTop="6dp"
            android:text="e-mail: info@affogato.pl"
            android:textSize="16dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.088"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView_stolik_tel" />

        <fragment
            android:id="@+id/map"
            android:name="com.google.android.gms.maps.SupportMapFragment"
            android:layout_width="412dp"
            android:layout_height="308dp"
            android:layout_marginEnd="1dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            tools:context=".MapsFragment2" />


    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>
